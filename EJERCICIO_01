<script type="text/javascript">
let tasks = [
	{	
	'name' : 'Buy milk from the shop',
	'duration' : 20 ,
	'priority' : 1
	},
	{
	'name' : 'Clean the house',
	'duration' : 120 ,
	'priority' : 3
	},
	{
	'name' : 'Study JS functions',
	'duration'  : 180 ,
	'priority' : 1
	}
];
function most(tasks){
	for (var i = 0; i <tasks.length; i++) {
	   console.log("duration: ",tasks[i].duration);
	}
};
most(tasks);

var mostrar=tasks.map(x=>x.duration);
console.log("duration: ",mostrar)

</script>
